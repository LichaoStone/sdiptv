<IMG src="Images/verify.png"></A -->

<input type="hidden"  id="msg" value="$!{message}"/>
						<em id="tip_message">
							$!{message}
                        </em>
<INPUT id="loginSubmit" class="btn" value="" type="submit"/>

</FORM>
